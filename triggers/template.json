{
    "name": "template",
    "type": "DATABASE",
    "disabled": false,
    "config": {
        "collection": "workflowtemplates",
        "database": "production",
        "full_document": true,
        "full_document_before_change": true,
        "match": {
            "$or": [
                {
                    "operationType": { "$eq": "insert" }  
                },
                {
                    "operationType": { "$eq": "update" }
                }
            ]
        },
        "maximum_throughput": false,
        "operation_types": [
            "INSERT", 
            "UPDATE"
        ],
        "project": {},
        "service_name": "ve-prod",
        "skip_catchup_events": false,
        "tolerate_resume_errors": false,
        "unordered": false
    },
    "event_processors": {
        "FUNCTION": {
            "config": {
                "function_name": "templates/template"
            }
        }
    }
}
